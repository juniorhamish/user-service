    "express-oauth2-jwt-bearer": "^1.7.4",
    "express-openapi-validator": "^5.6.2",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.1",
    "jose": "^6.1.3",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@tsconfig/node24": "^24.0.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.2.3",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "globals": "^17.3.0",
