{
  "name": "user-service",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "type-check": "tsc --noEmit",
    "start": "tsx src/index.ts",
    "dev": "tsx --watch src/index.ts",
    "test": "vitest run --coverage",
